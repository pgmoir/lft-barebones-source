        @{
            dynamic content = new ContentModel();

            content.ConfirmationHeadingSuccess = "We're now processing your 9+ seater discount renewal application";
            content.CompletedConfirmationHeading = "9+ seater discount renewal complete";
            content.CompletedConfirmationText = "<p>COMPLETEDCONFIRMATIONTEXT (9+ SEATER)</p>";
            content.ReferenceNumber = "Your 9+ seater discount renewal reference number is: ";
            content.ConfirmationText = "<p>We will send a confirmation if your renewal has been approved.</p><p>We aim to respond to you within the next 10 working days.</p>";
            content.PaymentReferenceText = "Renewal payment receipt number: {0}";
            content.EmailAddressSentToPrefixText = "Confirmation email sent to: {0}";
            content.SMSConfirmationText = "A receipt has been sent to mobile number: {0}";
            content.ExpiryDateText = "New discount expiry date: {0}";
            content.FinishButtonText = "Finish";
            
            content.PayCcTitle = "Travelling in the next few days?";
            content.PayCcText = "You need to pay the Congestion Charge for journeys within the zone. If your residents' discount has expired you will need to pay the charge in full until your discount has been approved.";
            content.PayCcLinkTitle = "Pay the charges"; 

            content.AutoPayApplyTitle = "Set up Auto Pay";
            content.AutoPayApplyText = "<p>Sign up and save. It's the easiest way to pay the Congestion Charge and Ultra Low Emission Zone.</p>";
            content.AutoPayApplyButtonText = "Set up Auto Pay";
            content.AutoPayManageTitle = "Update my Auto Pay";
            content.AutoPayManageText = "<p>Add your vehicle to your Auto Pay service.</p>";
            content.AutoPayManageButtonText = "Manage Auto Pay";

            Html.RenderAction("SubmissionSuccess", "DiscountRenew", new
                {
                    area = "Ruc",
                    discountStyle = ZoneChargeDiscountStyle.NinePlusSeat,
                    discountAction = DiscountAction.Renew,
                    content = content,
                    manageAccountUrl = "/modes/driving/account-account-landing-29377",
                    journeyStartUrl = "/modes/driving/cc-discount-manage-discounts-32822"
                }
            );
        }

       <h3>What happens next?</h3>
