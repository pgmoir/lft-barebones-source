<div class="r">
  <div class="headline-container plain">
    <h1>Vehicle history</h1>

  </div>
</div>
<div class="main-and-aside r">
  
  <div class="main content-container" role="main">
    
      <div class="article-teaser">
        <p>You can filter the results of your vehicle history to view vehicles that have been active on a discount or service, or narrow down your search by date range.</p>
        
      </div>
    
    
      
      
           
      

        
        <p>
         
        </p>
          
        @{
            dynamic content = new ContentModel();
            content.SearchLabelText = "Search by number plate";
            content.SearchPlaceholderText = "eg AB01ABC";
            content.SortLabelText = "Sort";
            content.ApplyFilterButtonText = "Apply filter";
            content.ResetFilterButtonText = "Reset filter";
            content.AutoPaySelectBoxText = "Select service";
            content.StartDateText = "Start date";
            content.EndDateText = "End date";
            content.MoreVehiclesTextMessage = "Total records: {0}";
            content.OneVehicleTextMessage = "1 record";
            content.DownloadButtonText = "Download vehicle list";
            content.VehicleHistoryDetailUrl = "/modes/driving/cc-vehicle-history-details-37495";
            content.FilterResultStatusHeading = "Status";
            content.DiscountActiveText = "Active";

            Html.RenderAction("Index", "ViewVehiclesHistory", new
                {
                    area = "Ruc",
                    content = content
                }
            );
        }

        @FormHelpers.DottedSeparator()
        
        <h2>Manage my vehicles</h2>
        <p>To make any changes to your vehicles, use the links below.</p>

        <ul class="vertical-button-container stacked-cta-list">
            @ModulesHelpers.BoxedLinkListItem(
                href: "/modes/driving/account-quick-management-list-37474",
                id: "quick-manage-link",
                text: "Manage vehicles on Quick Management list"
            )

            @ModulesHelpers.BoxedLinkListItem(
                href: "/modes/driving/cc-autopay-manage-manage-29152",
                id: "manage-autopay-link",
                text: "Manage or apply for Auto Pay"
            )
            
            @ModulesHelpers.BoxedLinkListItem(
                href: "/modes/driving/cc-discount-manage-discounts-32822",
                id: "vehicle-history-link",
                text: "Manage vehicles on discount"
            )
            
        </ul>

        @ModulesHelpers.BackLink()
        
       
 
      
        
          
        
      
    
  </div>
  <div class="aside" role="complementary">

    

        @{
            dynamic userInfoBoxContent = new ContentModel();
            Html.RenderAction("UserInfoBox", "Account", new { Area = "Ruc", content = userInfoBoxContent, logOutUrl = "/modes/driving/congestion-charge" });
        }
        
      

 
    
    
    <div></div>
    
    <div data-set="sidenav-container" class="small-medium"></div>
