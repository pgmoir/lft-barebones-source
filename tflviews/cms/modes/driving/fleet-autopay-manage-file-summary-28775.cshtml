<div class="r">
  <div class="headline-container plain">
    <h1>Upload a new vehicle list</h1>

  </div>
</div>
<div class="main-and-aside r">
  
  <div class="main content-container" role="main">
    
    
      <h2>File summary</h2>
      
           
      

        @using TfL.Ruc.Common.ConfigSection
        @{
            dynamic content = new ContentModel();
            content.UploadedFileText = "Uploaded file";
            content.TotalNumberofVehiclesText = "Total number of vehicles on Fleet Auto Pay following this upload: ";
            content.TotalActiveOnServiceText = "Total number of vehicles on Fleet Auto Pay before upload:";            
            content.BackButtonText = "Back";
            content.ContinueButtonText = "Confirm";
            content.ConfirmText = "<p>If the new total number of vehicles is not what you were expecting, go back, check your file and re-upload. Remember you must include existing vehicles on your upload file to keep them on your Auto Pay service.</p><p>Click 'Confirm' to upload this file and replace your current Fleet Auto Pay vehicle list.</p>";
            content.ProcessNotStartedErrorText = "Could not upload file. Please try again.";
            
            Html.RenderAction(
                "FileSummary",
                "FileUpload",
                new
                {
                    area = "Ruc",
                    manageUrl = "/modes/driving/account-account-landing-29377",
                    uploadFileUrl = "/modes/driving/fleet-autopay-manage-upload-file-28768",
                    processFileUrl = "/modes/driving/fleet-autopay-manage-process-file-28780",
                    fileUploadOperation = FileUploadOperation.AutoPayFleet,
                    content = content
                }
            );
        }
      
 
      
        
          
        
      
    
  </div>
  <div class="aside" role="complementary">

    

        @{
            dynamic userInfoBoxContent = new ContentModel();
            Html.RenderAction("UserInfoBox", "Account", new { Area = "Ruc", content = userInfoBoxContent, logOutUrl = "/modes/driving/congestion-charge" });
        }
        
      

 
    
    
    <div></div>
    
    <div data-set="sidenav-container" class="small-medium"></div>
  </div>
</div>

 
        
        
        
