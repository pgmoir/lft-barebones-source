        @{
        
            dynamic content = new ContentModel();
        
            content.BackButtonText = "Back";
            content.ContinueButtonText = "Continue";
        
            content.VehiclesHeading = "Vehicle(s)";
            content.SeatCountText = "Seat count: ";
        
            content.TermsAndConditionsHeading = "Terms and conditions";
            content.TermsAndConditionsText = "<p>I confirm that I have read, understood and accept the <a href='/corporate/terms-and-conditions/pay-to-drive-in-london' target='_blank'>residents' discount terms and conditions</a></p>";
        
            content.AddressHeading = "Address";
            content.AddressText = "Address";
        
            content.ActiveDiscountText = "Active on: ";
        
            content.ReviewEditButtonText = "Edit";
        
            content.SkippedVehicleText = "You have not yet added a vehicle.";
        
            content.TermsAndConditionsAcceptedRequireTrue = "Accept the terms and conditions to continue";
        
            Html.RenderAction(
                RucDictionary.DiscountApply.ReviewApplicationActionName,
                "DiscountApply",
                new
                {
                    area = "Ruc",
                    discountStyle = ZoneChargeDiscountStyle.ResidentsDiscount,
                    content = content,
                    successUrl = "/modes/driving/cc-provisional-resident-success-33366",
                    failureUrl = "/modes/driving/cc-provisional-resident-failure-33408",
                    selectVehicleUrl = "/modes/driving/cc-provisional-resident-select-vehicle-33362",
                    enterDetailsUrl = "/modes/driving/cc-provisional-resident-check-zone-33363",
                    submissionUrl = "/modes/driving/cc-provisional-resident-submit-33399",
                    journeyStartUrl = "/modes/driving/cc-select-discount-29641",
                    isProvisionalResident = true
                }
            );
        }
        
        
        
      

      
        
          
        
      
    
