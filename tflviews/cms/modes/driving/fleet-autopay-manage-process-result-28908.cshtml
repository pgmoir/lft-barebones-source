      <h2>Confirmation</h2>
      
           
      

        @using TfL.Ruc.Common.ConfigSection
        @{
            dynamic content = new ContentModel();

            content.BulkUploadResultText = "Your upload is complete. There are now {0} vehicles on your Fleet Auto Pay service.";
            content.SummaryOfVehiclesText = "Upload summary";
            content.TotalVehiclesText = "Total vehicles active on Fleet Auto Pay: ";
            content.TotalAddedText = "New vehicles added: ";
            content.TotalUnchangedText = "Vehicles unchanged: ";
            content.TotalRemovedText = "Vehicles removed: ";
            content.RejectedText = "Vehicles not added (rejected): ";
            content.AddedWithWarningText = "Vehicles added with warning: ";            
            content.ViewFullUploadDetailsText = "View full upload report";
            content.FeePerVehicleText = "Registration charge per vehicle:";
            content.TotalFeeToBePaidText = "Total cost of registration charges to be added to next statement: ";
            content.VehicleListText = "Manage my Fleet Auto Pay";
            content.BillingSummaryHeading = "Charge summary";
            content.BillingSummaryContent = "BILLINGSUMMARYCONTENT";
            content.NextStatementDateText = "Your next statement date will be: ";
            content.AccountStatusText = "Account status: ";
            
            Html.RenderAction(
                "ProcessResult",
                "FileUpload",
                new
                {
                    area = "Ruc",
                    manageUrl = "/modes/driving/cc-autopay-manage-manage-29152",
                    vehicleListUrl = "/modes/driving/fleet-autopay-manage-vehicles-28948",
                    content = content,
                    fileUploadOperation = FileUploadOperation.AutoPayFleet,
                    feePervehicle = 10m
                }
            );
        }
      
 
      
        
          
        
      
    
  </div>
