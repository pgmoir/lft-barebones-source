      <h2>Payment failed</h2>
      
           
      

 


		
		<!-- 7. cc-payment-failure -->
		@{
            dynamic content = new ContentModel();
            content.ErrorTitle = "We were unable to process your payment";
            content.PaymentErrorText = "Your payment could not be processed, please try again later.";
            content.PaymentDeclinedText = "Your payment has failed.";
            content.PaymentNextStepInfo = "<p>Your Congestion Charge amendment has not been processed. If you still want to amend the charge you will need to go back to <a href='/modes/driving/cc-change-a-charge-find-a-charge-26478'>Amend a Congestion Charge</a>.</p>";

            Html.RenderAction(
            "PaymentFailure", 
            "ChangeZoneCharge", 
            new
                {
                    area = "Ruc", 
                    content = content,
                    zoneType = ZoneChargeZone.CongestionCharge
                }
            );
        }
		
	  

  
      
        
          
        
      
    
