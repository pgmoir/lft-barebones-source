        <!-- 8. resident-discount -->
        @{
            dynamic content = new ContentModel();

            content.BackButtonText = "Back";
            content.ContinueButtonText = "Continue";

            content.VehiclesHeading = "Vehicle";
            //content.SeatCountText = "Seat count:";

            //content.TermsAndConditionsHeading = "Terms and Conditions";
            //content.TermsAndConditionsText = "I confirm that I have read and understood and accept the <a href=\"/\" target=\"_blank\">Resident discount terms and conditions</a>";

            content.EvidenceHeading = "Uploaded documents";
            content.EvidenceByPostText = "You have not uploaded any documents.";

            content.PaymentHeading = "Payment summary";
            content.PayementText = "<p>You need to pay the £10 registration charge before your application can be considered. </p><p>'Continue' to make this payment.</p>";
            content.PaymentTotalText = "Total";

            content.AddressHeading = "Address";
            content.AddressText = "Address";

            content.ReviewEditButtonText = "Edit";
            content.ActiveDiscountText = "Active on: ";

            //content.SkippedVehicleText = "<p>You have not yet added a vehicle.</p>";
            content.NoVehicleText = "You have no vehicles on your discount.";
            
            //content.TermsAndConditionsAcceptedRequireTrue = "You must accept the terms and conditions.";

            Html.RenderAction("ReviewApplication", "DiscountRenew", new
                {
                    area = "Ruc",
                    discountStyle = ZoneChargeDiscountStyle.ResidentsDiscount,
                    discountAction = DiscountAction.Renew,
                    content = content,                    
                    enterDetailsUrl = "/modes/driving/cc-resident-discount-renew-check-zone-34059",
                    uploadEvidenceUrl = "/modes/driving/cc-resident-discount-renew-upload-evidence-34078",
                    selectVehicleUrl = "/modes/driving/cc-resident-discount-renew-check-vehicle-34057",                    
                    paymentUrl = "/modes/driving/cc-resident-discount-renew-payment-34063",
                    journeyStartUrl = "/modes/driving/cc-discount-manage-discounts-32822"
                }
            );
        }
        
	  

      
        
          
        
      
    
