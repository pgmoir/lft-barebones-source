		<!-- 8. resident-discount -->
        <p>If you change your vehicle as part of this renewal process you won't be able to pay the 90% discounted rate for your new vehicle until your renewal has been approved. This can take up to 10 working days.</p>

<p>If you need to change your vehicle before then, call us on 0343 222 2222 (<span class="external-link"><a href="/corporate/terms-and-conditions/tfl-call-charges">TfL call charges</a></span>).</p>
        @{
            dynamic content = new ContentModel();

            content.ContinueButtonText = "Continue";
            content.BackButtonText = "Back";
            content.ChangeVehicleLinkText = "Change vehicle";
            content.ActiveOnText = "Active on: ";
            content.NoVehicleWarningText = "You have not added a vehicle to your residents' discount";

            Html.RenderAction(
                RucDictionary.DiscountManage.ChangeAddVehicleActionName,
                "DiscountRenew",
                new
                {
                    area = "Ruc",
                    nextUrl = "/modes/driving/cc-resident-discount-renew-upload-evidence-34078",
                    backUrl = "/modes/driving/cc-resident-discount-renew-check-zone-34059",
                    selectVehicleUrl = "/modes/driving/cc-resident-discount-renew-select-vehicle-34070",
                    reviewUrl = "/modes/driving/cc-resident-discount-renew-review-application-34068",
                    content = content,
                    discountStyle = ZoneChargeDiscountStyle.ResidentsDiscount,
                    journeyStartUrl = "/modes/driving/cc-discount-manage-discounts-32822"
                }
            );
        }
		
		
	  


      
        
          
        
      
    
