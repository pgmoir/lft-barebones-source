                    travelContactFormViewModel = new TravelContactFormViewModel { Content = travelContactContent, OysterNumberVisible = true, 
                        DateOfTravelRequired = true, DateOfTravelVisible = true, TimeOfTravelRequired = true, TimeOfTravelVisible = true, 
                        DirectionOfTravelRequired = true, JourneyDetailsHeadingVisible = true },
                    formCommentsViewModel = new FormCommentsViewModel { Content = formCommentsContent },
                    formAttachmentsViewModel = new FormAttachmentsViewModel { FormName = "Emirates", FileCountLimit = 3 },                    
                    submission = new FormSubmissionViewModel { DataProtection = @dataProtectionText, SuccessfulSubmissionMessage = @successText, FailedSubmissionMessage = @failureText }
                });
                            
                }
                
                
                
